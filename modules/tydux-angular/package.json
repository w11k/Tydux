{
    "name": "tydux-demo",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "clean": "rimraf dist",
        "build": "ng build",
        "build:tydux": "ng build tydux",
        "build:tydux-angular": "ng build tydux-angular",
        "build:lib": "run-s build:tydux build:tydux-angular cp:license cp:readme",
        "start": "npm run start:demo",
        "start:demo": "ng serve",
        "start:tydux": "npm run build:tydux -- --watch",
        "start:angular": "npm run build:tydux-angular -- --watch",
        "test": "npm run clean && npm run build:lib && npm run test:all",
        "test:lib": "jest --config ./jest.lib.config.js",
        "test:lib:watch": "jest --watchAll --config ./jest.lib.config.js",
        "test:app": "jest --config ./jest.app.config.js",
        "test:all": "tsc; npm run test:lib && npm run test:app",
        "cp:license": "cpx LICENSE dist/w11k/tydux && cpx LICENSE dist/w11k/tydux-angular",
        "cp:readme": "cpx README.md dist/w11k/tydux",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "publish.tydux": "cd dist/w11k/tydux && npm publish",
        "publish.tydux-angular": "cd dist/w11k/tydux-angular && npm publish",
        "publish.all": "npm run publish.tydux && npm run publish.tydux-angular",
        "git.tag": "cd dist/w11k/tydux && git tag v`npm info . version`",
        "git.flow": "npm run git.tag && git push --tags && git push",
        "release": "npm test && npm run publish.all && npm run git.flow"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.0.0",
        "@angular/common": "~8.0.0",
        "@angular/compiler": "~8.0.0",
        "@angular/core": "~8.0.0",
        "@angular/forms": "~8.0.0",
        "@angular/platform-browser": "~8.0.0",
        "@angular/platform-browser-dynamic": "~8.0.0",
        "@angular/router": "~8.0.0",
        "@w11k/ngx-componentdestroyed": "^4.1.4",
        "@w11k/rx-ninja": "^4.1.0",
        "core-js": "^2.5.4",
        "redux": "4.x.x",
        "redux-devtools-extension": "2.13.8",
        "rxjs": "~6.5.2",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.800.0",
        "@angular-devkit/build-ng-packagr": "~0.800.0",
        "@angular/cli": "~8.0.1",
        "@angular/compiler-cli": "~8.0.0",
        "@angular/language-service": "~8.0.0",
        "@types/chai": "4.1.2",
        "@types/es6-shim": "0.31.35",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "babel-core": "^6.26.3",
        "chai": "4.1.2",
        "codelyzer": "^5.0.1",
        "cpx": "^1.5.0",
        "get-version": "1.1.3",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^23.1.0",
        "jest-preset-angular": "^6.0.2",
        "ng-mocks": "~8.0.0",
        "ng-packagr": "^5.2.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^11.4.1",
        "protractor": "~5.4.0",
        "rimraf": "3.0.2",
        "ts-node": "~7.0.0",
        "tsickle": "0.37.1",
        "tslint": "~5.11.0",
        "typescript": "3.4.5"
    }
}
