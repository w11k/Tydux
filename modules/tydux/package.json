{
    "name": "@w11k/tydux",
    "version": "16.1.3",
    "author": "W11K GmbH",
    "contributors": [
        "Roman Roelofsen",
        "Philipp Burgmer",
        "Sascha Engmann",
        "Mario Sallat"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/w11k/Tydux",
    "repository": {
        "type": "git",
        "url": "https://github.com/w11k/Tydux.git"
    },
    "files": [
        "dist"
    ],
    "np": {
        "yarn": false
    },
    "typings": "./dist/bundle/index.d.ts",
    "main": "./dist/bundle/index.js",
    "module": "./dist/esm5/index.js",
    "esm5": "./dist/esm5/index.js",
    "esm2015": "./dist/esm2015/index.js",
    "scripts": {
        "build": "run-s clean test compile build:copy_from_root",
        "build:copy_from_root": "shx cp ../../README.md ../../LICENSE .",
        "clean": "shx rm -rf dist",
        "compile": "run-p compile:*",
        "compile:base": "tsc -p tsconfig.json",
        "compile:esm2015": "tsc -p tsconfig.dist.esm2015.json",
        "compile:esm5": "tsc -p tsconfig.dist.esm5.json",
        "np": "np",
        "prepublishOnly": "run-s build",
        "release": "run-s build np",
        "test": "jest --config ./jest.config.js",
        "test:watch": "jest --config ./jest.config.js --watchAll"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@w11k/rx-ninja": "^4.0.0"
    },
    "peerDependencies": {
        "immer": ">= 9.0.0",
        "redux-devtools-extension": "^2.13.9",
        "rxjs": ">= 6.2.0",
        "typescript": "^3 || ^4"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.12.16",
        "@babel/preset-typescript": "^7.13.0",
        "@types/jest": "^26.0.20",
        "babel-jest": "^26.6.3",
        "immer": "^10.0.1",
        "jest": "^26.6.3",
        "np": "^7.6.1",
        "npm-run-all": "^4.1.5",
        "redux-devtools-extension": "^2.13.9",
        "rxjs": ">= 6.2.0",
        "shx": "^0.3.3",
        "tslib": "^2.3.1",
        "tslint": "~6.1.3",
        "typescript": "^4.5.4"
    }
}
