{
    "name": "@w11k/tydux",
    "version": "15.0.0",
    "author": "W11K GmbH",
    "contributors": [
        "Roman Roelofson",
        "Philipp Burgmer",
        "Sascha Engmann"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/w11k/Tydux",
    "repository": {
        "type": "git",
        "url": "https://github.com/w11k/Tydux.git"
    },
    "files": [
        "bundle",
        "esm5",
        "esm2015",
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "np": {
        "contents": "dist"
    },
    "typings": "./bundle/index.d.ts",
    "main": "./bundle/index.js",
    "module": "./esm5/index.js",
    "esm5": "./esm5/index.js",
    "esm2015": "./esm2015/index.js",
    "scripts": {
        "clean": "shx rm -rf dist",
        "test": "jest --config ./jest.config.js",
        "test:watch": "jest --config ./jest.config.js --watchAll",
        "compile": "run-p compile:*",
        "compile:base": "tsc -p tsconfig.json",
        "compile:esm5": "tsc -p tsconfig.dist.esm5.json",
        "compile:esm2015": "tsc -p tsconfig.dist.esm2015.json",
        "build:copy_from_root": "shx cp ../../CHANGELOG.md ../../README.md ../../LICENSE dist",
        "build": "run-s clean test compile build:copy_from_root",
        "prepublishOnly": "run-s build"
    },
    "dependencies": {
        "redux": "4.x.x",
        "@w11k/rx-ninja": "^4.0.0"
    },
    "peerDependencies": {
        "rxjs": ">= 6.2.0",
        "redux": "^4.0.0",
        "redux-devtools-extension": "2.13.8"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.12.16",
        "@babel/preset-typescript": "^7.13.0",
        "babel-jest": "^26.6.3",
        "@babel/core": "^7.13.10",
        "tslib": "^2.1.0",
        "rxjs": ">= 6.2.0",
        "redux-devtools-extension": "2.13.8",
        "typescript": "^4.2.3",
        "@types/jest": "^26.0.20",
        "jest": "^26.6.3",
        "np": "^7.4.0",
        "npm-run-all": "^4.1.5",
        "shx": "^0.3.3",
        "tslint": "~5.11.0"
    }
}
