{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "strict": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es5",
      "es2015.proxy",
      "dom"
    ],
    "rootDir": "src",
    "target": "es5",
    "outDir": "dist"
  }
}
